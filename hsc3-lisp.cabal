Name:              hsc3-lisp
Version:           0.19
Synopsis:          LISP SUPERCOLLIDER
Description:       LISP SUPERCOLLIDER
License:           GPL
Category:          Sound
Copyright:         (c) Rohan Drape, 2014-2020
Author:            Rohan Drape
Maintainer:        rd@rohandrape.net
Stability:         Experimental
Homepage:          http://rohandrape.net/t/hsc3-lisp
Tested-With:       GHC == 8.8.3
Build-Type:        Simple
Cabal-Version:     >= 1.8

Data-files:        README

executable hsc3-lisp
  build-depends:   base >= 4.8,
                   containers,
                   directory,
                   filepath,
                   hashable,
                   husk-scheme >= 3.19.4,
                   hosc == 0.19.*,
                   hsc3 == 0.19.*,
                   hsc3-dot == 0.19.*,
                   hsc3-rw == 0.19.*,
                   mtl,
                   safe,
                   unix
  hs-source-dirs:  hs
  other-modules:   Lisp,
                   Lisp.Type,
                   Lisp.Parse.Ethier
  main-is:         hsc3-lisp.hs
  ghc-options:     -Wall -fno-warn-orphans -fwarn-tabs

Source-Repository  head
  Type:            darcs
  Location:        http://rohandrape.net/sw/hsc3-lisp/
